@model KutuphanePlatformu.ViewModels.MesajViewModel
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
@if (Model.Status)
{
    <div class="alert alert-success">
        @Model.Mesaj <a href="@Model.Url">@Model.LinkText</a>
    </div>
}
else
{
    <div class="alert alert-danger">
        @Model.Mesaj <a href="@Model.Url">@Model.LinkText</a>
    </div>
}
