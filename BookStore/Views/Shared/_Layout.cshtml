<!DOCTYPE html>
<html lang="tr">
<head>
    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>MAS-Kitap Satış</title>

    <meta name="keywords" content="Kitap, Satış">
    <meta name="description" content="MAS-Kitap Satış">
    <meta name="author" content="MAS">

    <!-- Mobile Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:400,500,600,700" rel="stylesheet">

    <!-- Vendor CSS -->

    <link rel="stylesheet" href="~/Content/libs/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/libs/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/libs/font-material/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/Content/libs/nivo-slider/css/nivo-slider.css">
    <link rel="stylesheet" href="~/Content/libs/nivo-slider/css/animate.css">
    <link rel="stylesheet" href="~/Content/libs/nivo-slider/css/style.css">
    <link rel="stylesheet" href="~/Content/libs/owl-carousel/assets/owl.carousel.min.css">


    <!-- Template CSS -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/reponsive.css">
    <!--Custom CSS-->
    <link href="~/Content/css/CustomStyle.css" rel="stylesheet" />
</head>

<body id="home">
    <header>
        <!-- header left mobile  ->> Cart ->>Account -->
        @Html.Partial("_MobileLeftHeader")


        <!-- header desktop ->> Cart ->> _Account -->
        @Html.Partial("_DesktopHeader")

    </header>

    <!-- main content -->
    @RenderBody()


    <!-- footer -->
    @Html.Partial("_Footer")


    <!-- back top top -->
    <div class="back-to-top">
        <a href="#">
            <i class="fa fa-long-arrow-up"></i>
        </a>
    </div>

    <!-- menu mobile left -->
    @Html.Partial("_MobileLeftMenu")


    <!-- menu mobile right -->
    @Html.Partial("_MobileRightMenu")

    <!-- Page Loader -->
    <div id="page-preloader">
        <div class="page-loading">
            <div class="dot"></div>
            <div class="dot"></div>
            <div class="dot"></div>
            <div class="dot"></div>
            <div class="dot"></div>
        </div>
    </div>
    <!-- Vendor JS -->
    <script src="~/Content/libs/jquery/jquery.min.js"></script>
    <script src="~/Content/libs/popper/popper.min.js"></script>
    <script src="~/Content/libs/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/libs/nivo-slider/js/jquery.nivo.slider.js"></script>
    <script src="~/Content/libs/owl-carousel/owl.carousel.min.js"></script>

    <!-- Template JS -->
    <script src="~/Content/js/theme.js"></script>
</body>



</html>